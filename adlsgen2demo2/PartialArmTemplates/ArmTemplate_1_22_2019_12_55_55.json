{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adlsgen2demo2"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/DemoBlobData')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "AzureBlobStorage",
				"typeProperties": {
					"sasUri": "https://adlsgen2demo3.blob.core.windows.net/demo-data?st=2019-01-22T20%3A39%3A53Z&se=2022-01-23T20%3A39%3A00Z&sp=rl&sv=2018-03-28&sr=c&sig=sj5jVasVagBWw7qyqmAgqyGReLGP%2FETNGmdK5tcr15Q%3D"
				}
			}
		}
	]
}